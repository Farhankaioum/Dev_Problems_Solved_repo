@model List<User> 

@{
    ViewData["Title"] = "Home Page";
}

<div class="container">
    <div class="text-center">
        <h1 class="display-4">Welcome</h1>
    </div>

    <div class="row">
        <table class="table">
            <tr>
                <th>Name</th>
                <th>Address</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Address.AreaName</td>
                </tr>
            }
        </table>
       
    </div>
</div>
